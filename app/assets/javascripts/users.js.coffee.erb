$(window).load ->
  if $("body").hasClass "users"
    enchant()
    if $("body").hasClass "index"
      game = new Core(640, 480)
      game.rootScene.backgroundColor = "green"
      img = ["<%= asset_path("flash.png") %>"]
      game.preload(img[0])
      game.onload = ->
        main_sprite = new Sprite(32, 32)
        main_sprite.image = game.assets[img[0]]
        game.rootScene.addChild main_sprite
        rot = 0
        game.addEventListener "enterframe", ->
          rot += 0.02
          main_sprite.x = 304 + Math.cos(rot) * 100
          main_sprite.y = 224 + Math.sin(rot) * 100
          return
        return
      game.start()

